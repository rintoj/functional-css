  },
  "homepage": "https://github.com/rintoj/functional-css#readme",
  "devDependencies": {
    "@types/chai": "^3.5.2",
    "@types/mocha": "^2.2.41",
    "@types/node": "^7.0.21",
    "awesome-typescript-loader": "^3.1.3",
    "chai": "^3.5.0",
    "chai-spies": "^0.7.1",
    "concurrently": "^3.4.0",
    "css-loader": "^0.28.1",
    "eslint": "^3.19.0",
    "extract-text-webpack-plugin": "^2.1.0",
